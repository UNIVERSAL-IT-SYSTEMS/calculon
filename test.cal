let quadratic(a, b, c):vector =
	let f = sqrt(b*b - 4*a*c);
	let s1 = (-b + f) / (2*a);
	let s2 = (-b - f) / (2*a);
	[s1, s2, 0];

let fibonacci(n) =
	let fib2(n, p0, p1) =
		if (n <= 1) then
			p1
		else
			fib2(n-1, p1, p0+p1);
	fib2(n, 0, 1);

let sum(n) =
	let sum2(n, p) =
		if (n <= 1) then
			p
		else
			sum2(n-1, p+n);
	sum2(n, 1);
	
